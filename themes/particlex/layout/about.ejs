<div class="article">
  <div>
    <h1 style="color: #ff7d73"><%= page.title %></h1>
  </div>
  <div class="info">
    <span class="date">
      <span class="icon">
        <i class="fa-solid fa-calendar fa-fw"></i>
      </span>
      <%= date(page.date, "YYYY/M/D") %>
    </span>
  </div>
  <div class="content" v-pre>
    <%- page.content %>
  </div>
  <div></div>
</div>
